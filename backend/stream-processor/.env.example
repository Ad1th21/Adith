NODE_ENV=development
LOG_LEVEL=info

# Redis
REDIS_URL=redis://localhost:6379

# PostgreSQL
DATABASE_URL=postgresql://telemetry_user:telemetry_pass@localhost:5432/telemetry

# Stream configuration
CONSUMER_GROUP=telemetry-processors
CONSUMER_NAME=processor-1
BATCH_SIZE=10
BLOCK_MS=5000

# Alert thresholds
ALERT_LOW_BATTERY_THRESHOLD=20
ALERT_OVERSPEED_THRESHOLD=120
ALERT_HIGH_TEMPERATURE_THRESHOLD=60
